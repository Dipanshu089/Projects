 char name[]="Hello World!!";
  const char* name;
 for(int i=1;i<=n;i++)//similarly code can be written for while loop
  cout<<name<<endl;
 //using do while
  char name[]="Hello World!!";
 int i=1;
 do
 {
  cout<<name<<endl;
  i++;
 }while(i<=n); 
