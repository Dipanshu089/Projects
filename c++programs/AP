#include <iostream>
#include <cmath>
using namespace std;

int main()
{int x,y;
	int a;
	cin>>a;
	for(int b=0;b!=a;++b)

    cout << "Enter an integer: ";
    cin >>x>>y;
    int c=0;
    int d;
    int e=0;
    int f;
    while(x != 0,y!=0)
    {
        d = x%10;
        c = c*10 + d;
        x /= 10;
        f = y%10;
        e = e*10 + f;
        y /= 10;
    }

    cout << "Reversed Number = " << c;

    return 0;
}
