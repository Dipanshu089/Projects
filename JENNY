}
void sumnatural()
{
 int n,sum;
 cout<<"Enter a Number to Find the Sum: ";
 cin>>n;
 sum=(n*(n+1))/2;
 cout<<"Sum is"<<sum;

}
void vowelconst()
{
 char c;
 cin>>c;
 int islowercase,isuppercase;
 islowercase = (c=='a'||c=='e'||c=='i'||c=='o'||c=='u');
 isuppercase = (c=='A'||c=='E'||c=='I'||c=='O'||c=='U');
 if(islowercase || isuppercase)
   cout<<"Alphabet is Vowel";
 else
   cout<<"Alphabet is Consonant";
}
