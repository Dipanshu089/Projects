 }
}
void multiplicationtable()
{
 int n;
 cout<<"Enter number N of which you want table: ";
 cin>>n;
 for(int i=1,i<11,i++)
 {
  cout<<n<<"*"<<i<<" = "<<n*i;
 }
}
void quadratic()
{
 float a,b,c,x1,x2,sum,multi;//a!=0
 cout<<"Enter the a and b of quadratic equation: ";
 cin>>a>>b;
 sum=-b/a;
 multi=c/a;
 x1=(-b+sqrt(b*b-4*a*c))/2;x2=(-b-sqrt(b*b-4*a*c))/2;
 cout<<"Sum of roots is"<<sum;
 cout<<"Multiplication of roots is"<<multi;
 cout<<"Roots of quadratic Equation are"<<x1<<x2;
 if(b*b>4*a*c)
  cout<<"Real Roots both";
 else if(b*b<4*a*c)
  cout<<"Complex Roots both";
 else
  cout<<"Single root";//anothrt condition if deternament is a square
 scanf("Different Representation => %d(x-%d)(x-%d)=0",a,x1,x2);
}
void sequence()
{int n;
 cin>>n;
 switch(n)
 {
 case 1://aritmetic progression
        int a(n),a,n,d,sum;
        a(n)=a+(n-1)*d;
        cout<<"Enter the first Number: ";
        cin>>a;
        cout<<"Enter the Difference: ";
        cin>>d;  
        cout<<"Enter the N: ";
        cin>>n;
        cout<<"Last Number is "<<a(n);
        sum=(n*(2*a+(n-1)*d))/2;
        cout<<"Sum is "<<sum;
        cout<<"Arithmetic Mean is "<<sum/n;
        break;
 case 2://geometric progression
        int a(n),a,r,n;
        cout<<"Enter the First Number: ";
        cin>>a;
        cout<<"Enter the Common Ratio: ";
        cin>>r;
        cout<<"Enter the N: ";
        cin>>n;
        a(n)=a*pow(r,n-1);
        cout<<"Last Number is: "<<a(n);
        a(n)=a;
        for(int i=1,i<n,i++);
        {
         cout>>a*
        }
 case 3://harmonic progression
        int a(n),a,n;
        
        
        
}
int main()
{
 int n;
 cin>>n;
 switch(n)
 {
  case 1:add(); break;//done
  case 2:subtract();break;//done
  case 3:multiply();break;//done
  case 4:divide();break;//done
  case 5:remainder();break;//done
  case 6:comparetwo();break;//done
  case 7:comparethree();break;//done
  case 8:leapyear();break;//done
  case 9:areas();break;
  case 10:volume();break;
  case 11:sequence();break;//incomplete
  case 12:quadratic();break;//near done
  case 13:permutation();break;
  case 14:probablity();break;
  case 15:stats();break;
  case 16:vector3d();break;
  case 17:trignometry();break;
  case 18:diffcalculus();break;

  case 19:intcalculus();break;
  case 20:matricesdet();break;
  case 21:geometry();break;
  case 22:evenodd();break;//done
  case 23:prime();break;
  case 24:sumnatural();break;//done
  case 25:vowelconst();break;//done
  case 26:multiplicationtable();break;//done
  case 27:graph();break;
  case 28:coordinategeometry();break;
 }
}
