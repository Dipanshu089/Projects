void areas()
{
 int n;
 cout<<"NO of Sides: ";
 cin>>n;
 switch(n)
 {
  case 1:trapezium();break;
  case 3:triangle();break;
  case 4:square();break;
  case 5:pentagon();break;
  case 6:hexagon();break;
  case 7:heptagon();break;
  case 8:octagon();break;
  case 9:nonagon();break;
  case 10:decagon();break; 
  case 11:ellipse();break;
 }
}
