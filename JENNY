void add()
{
 int a,b,sum;
 printf("Want To Add Some Numbers: ");
 scanf("%d + %d",&a,&b);
 sum=a+b;
 printf("Sum is %d",sum);
}
void subtract()
{
 int a,b,subtract;
 printf("Want To Subtract Some Numbers: ");
 scanf("%d - %d",&a,&b);
 subtract=a-b;
 printf("Subtract is %d",subtract);
}

void multiply()
{
 int a,b,multiply;
 printf("Want To Multiply Some Numbers: ");
 scanf("%d * %d",&a,&b);
 multiply=a*b;
 printf("Multiply is %d",multiply);
}
void divide()
{
 int a,b,divide;
 printf("Want To Divide Some Numbers: ");
 scanf("%d / %d",&a,&b);
 divide=a/b;
 printf("Divide is %d",divide);
}
void remainder()
{
 int a,b,remainder;
 printf("Want To Find Remainderof Some Numbers: ");
 scanf("%d % %d",&a,&b);
 remainder=a%b;
 printf("Remainder is %d",remainder);
}
void comparetwo()
{
 int a,b;
 if(a>b)
   cout>>a>>"is greater than">>b;
 else
   cout>>b>>"is greater than">>a;
}
void comparethree()
{
 int a,b,c;
 if(a>b && a>c)
   cout>>a>>"is greatest";
 else if(b>c && b>a)
   cout>>b>>"is greatest";
 else
   cout>>c>>"is greatest";
}
void leapyear()
{
     int year;
    cout << "Enter a year: ";
    cin >> year;
    if (year % 4 == 0)
    {
        if (year % 100 == 0)
        {
            if (year % 400 == 0)
                cout << year << " is a leap year.";
            else
                cout << year << " is not a leap year.";
        }
        else
            cout << year << " is a leap year.";
    }
    else
        cout << year << " is not a leap year.";
}
void evenodd()
{
 int a;
 if(a%2 == 0)
   cout<<"Number is Even";
 else
   cout<<"Number is Odd";
}
